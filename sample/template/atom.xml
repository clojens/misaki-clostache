<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>{{site-title}}</title>
    <link rel="self" href="{{atom-base}}{{url-base}}" />
    <link href="{{atom-base}}{{url-base}}" />
    <updated>{{date-xml-schema}}</updated>
    <id>{{atom-base}}</id>
    <author>
        <name>{{twitter}}</name>
    </author>

    {{#posts}}
    <entry>
        <title>{{title}}</title>
        <link>{{atom-base}}{{url}}</link>
        <updated>{{date-xml-schema}}</updated>
        <id>{{atom-base}}{{url}}</id>
        <content type="html">{{content}}</content>
    </entry>
    {{/posts}}
</feed>
